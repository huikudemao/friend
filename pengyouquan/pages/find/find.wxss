/* pages/find/find.wxss */
.lines{
    width: 100%;
    height: 1rpx;
    border-top: 1rpx solid #e0e0e0;
    margin-top: 25rpx;
    margin-bottom: 25rpx;
}
.find-ser{
    display: flex;
    height: 90rpx;
}
.find-input{
    border:solid 1rpx rgb(121, 121, 121);
    height:78rpx;
    flex: 1;
    margin-left: 20rpx;
    margin-right: 20rpx;
    border-radius: 10rpx;
}
.find-btn{
    height: 80rpx;
    margin-right: 20rpx;
}
.find-content{
    display: flex;
}
.find-view1{
    flex: 1;
    margin-left: 30rpx;
}
.find-view2{
    color: rgb(139, 139, 139);
    font-size: 33rpx;
    margin-top: 20rpx;
    display: flex;
}
.find-view3{
    margin-left: 20rpx;
    font-size: 32rpx;
    margin-top: 5rpx;
}
.find-img{
    height: 160rpx;
    width: 160rpx;
    margin-right: 20rpx;
    margin-left: 10rpx;
}